{
  "name": "marijnvdwerf/material-palette",
  "description": "Prominent image colour extraction for PHP",
  "homepage": "https://github.com/marijnvdwerf/material-palette-php",
  "keywords": [
    "colour",
    "color",
    "material",
    "material-design",
    "material design",
    "image",
    "palette"
  ],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Marijn van der Werf",
      "email": "mail@marijnvdwerf.nl",
      "homepage": "http://marijnvdwerf.nl"
    }
  ],
  "autoload": {
    "psr-4": {
      "marijnvdwerf\\palette\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "marijnvdwerf\\palette\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.3",
    "intervention/image": "~2.2"
  },
  "suggest": {
    "ext-gd": "to use the GD implementation",
    "ext-imagick": "to use the Imagick implementation"
  },
  "require-dev": {
    "devster/ubench": "^1.1",
    "phpunit/phpunit": "^6.1",
    "symfony/debug": "~2.6",
    "symfony/var-dumper": "~2.6"
  }
}
